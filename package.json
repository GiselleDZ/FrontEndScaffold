{
  "name": "front-end-scaffold",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Giselle Zatonyl <gisellezatonyl@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "heroku-prebuild": "npm install --dev",
    "build": "webpack --mode production",
    "dev": "webpack-dev-server --mode development",
    "devserver": "live-server",
    "watch:sass": "node-sass public/sass/main.scss public/css/style.css -w",
    "start:dev": "npm-run-all --parallel dev watch:sass",
    "compile:sass": "node-sass public/sass/main.scss public/css/style.css",
    "prefix:css": "postcss public/css/style.css --use autoprefixer -o public/css/style.prefix.css",
    "compress:css": "node-sass public/css/style.prefix.css public/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "build:all": "npm-run-all --parallel build:css build ",
    "heroku-postbuild": "webpack -p"
  },
  "browserslist": [
    "last 10 versions",
    "> 5%",
    "IE 10"
  ],
  "dependencies": {
    "express": "^4.17.1",
    "i": "^0.3.6",
    "npm": "^6.14.8",
    "react": "^16.9.0",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.6.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.6.0",
    "@svgr/webpack": "^4.3.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "axios": "^0.20.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "css-loader": "^3.2.0",
    "eslint": "^7.10.0",
    "eslint-plugin-react": "^7.21.2",
    "express": "^4.17.1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "2.1.2",
    "redux": "^4.0.5",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.1.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  }
